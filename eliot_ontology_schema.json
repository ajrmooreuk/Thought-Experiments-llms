  "eliotSpecificTerminology": {
    "@id": "literary:EliotTerminology",
    "@type": "DefinedTermSet",
    "name": "Eliot-Specific Critical Terminology",
    "hasDefinedTerm": [
      {
        "@id": "literary:ObjectiveCorrelative",
        "@type": "DefinedTerm",
        "name": "Objective Correlative",
        "description": "A set of objects, situations, or chain of events that evoke a particular emotion; formulated by Eliot in 'Hamlet and His Problems' (1919)",
        "termCode": "OC",
        "inDefinedTermSet": "literary:EliotTerminology"
      },
      {
        "@id": "literary:TraditionAndIndividualTalent",
        "@type": "DefinedTerm",
        "name": "Tradition and Individual Talent",
        "description": "Eliot's concept of how individual poets relate to literary tradition, emphasizing impersonality and historical sense",
        "termCode": "TIT",
        "inDefinedTermSet": "literary:EliotTerminology"
      },
      {
        "@id": "literary:DissociationOfSensibility",
        "@type": "DefinedTerm",
        "name": "Dissociation of Sensibility",
        "description": "Eliot's theory that a split between thought and feeling occurred in English poetry after the metaphysical poets",
        "termCode": "DOS",
        "inDefinedTermSet": "literary:EliotTerminology"
      },
      {
        "@id": "literary:MythicMethod",
        "@type": "DefinedTerm",
        "name": "Mythic Method",
        "description": "Use of myth and literary/religious allusion to order and give shape to contemporary chaos, as identified by Eliot in Joyce's Ulysses",
        "termCode": "MM",
        "inDefinedTermSet": "literary:EliotTerminology"
      }
    ]
  },
